{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Frontend",
			"request": "launch",
			"type": "chrome",
			"url": "http://localhost:8083",
			"webRoot": "${workspaceFolder}/server",
			"skipFiles": [
				"<node_internals>/**",
				"**/*.min.js",
				"**/vendor/**"
			],
		},
		{
			"name": "Data:stations",
			"program": "${workspaceFolder}/datagenerators/stations.js",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node"
		},
		{
			"name": "Data:regionalcities",
			"program": "${workspaceFolder}/datagenerators/regionalcities.js",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Server",
			"skipFiles": [
				"<node_internals>/**",
			],
			"program": "${workspaceFolder}/index.js",
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Server-dist",
			"skipFiles": [
				"<node_internals>/**",
			],
			"program": "${workspaceFolder}/index.js",
			"env": {
				"DIST": "1"
			}
		},
		{
			"name": "Test",
			"program": "${workspaceFolder}/tests/index.js",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node",
			"outputCapture": "std"
		},
	],
	"compounds": [
		{
			"name": "Compound",
			"configurations": [
				"Frontend",
				"Server"
			]
		}
	]
}