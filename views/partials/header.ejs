<div class="header">
	<div class="logo"><img src="images/Logo3.png" /></div>
	<% if (locals?.titleDual) { %>
		<div class="title dual">
			<div class="top">
				<%-titleDual.top %>
			</div>
			<div class="bottom">
				<%-titleDual.bottom %>
			</div>
		</div>
		<% } else { %>
			<div class="title single">
				<%-title %>
			</div>
			<% } %>
</div>